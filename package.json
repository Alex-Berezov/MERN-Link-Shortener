{
  "name": "mern-link-shortener",
  "version": "1.0.0",
  "description": "MERN stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Berezov/MERN-Link-Shortener.git"
  },
  "keywords": [
    "mern",
    "Mongo",
    "Express",
    "React",
    "Node"
  ],
  "author": "Alex Berezov <aleber197@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alex-Berezov/MERN-Link-Shortener/issues"
  },
  "homepage": "https://github.com/Alex-Berezov/MERN-Link-Shortener#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "create-react-app": "^4.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.11.15",
    "node": "^15.8.0",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7"
  }
}
